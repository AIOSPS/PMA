<!DOCTYPE hibernate-mapping SYSTEM "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-lazy="false">

    <!--User Mapping-->
    <class
            name="eu.pms.login.database.SecRole"
            table="sec_role"
    >
        <id
                name="rolId"
                type="int"
                column="ROL_ID"
        >
            <generator class="assigned"/>
        </id>
        <property
                name="rolName"
                type="java.lang.String"
                column="ROL_NAME"
                not-null="true"
                length="50"
        />
        <property
                name="rolADesc"
                type="java.lang.String"
                column="ROL_A_DESC"
                length="100"
        />
        <property
                name="rolLDesc"
                type="java.lang.String"
                column="ROL_L_DESC"
                length="100"
        />
        <property
                name="rolStatus"
                type="java.lang.String"
                column="ROL_STATUS"
                length="2"
        />
        <property
                name="rolCat"
                type="java.lang.String"
                column="ROL_CAT"
                length="10"
        />

        <!-- associations -->
        <!-- bi-directional one-to-many association to SecUserRole -->
        <set
                name="secUserRoles"
                lazy="true"
                inverse="true"
        >
            <key>
                <column name="ROL_ID"/>
            </key>
            <one-to-many
                    class="eu.pms.login.database.SecUserRole"
            />
        </set>

    </class>
    <class name="eu.pms.login.database.SecUser" table="sec_user" >
        <id name="usrId" type="java.lang.String" column="USR_ID" >
            <generator class="assigned"/>
        </id>
        <property name="usrPassword" type="java.lang.String" column="USR_PASSWORD" />
        <property name="usrStatus" type="java.lang.String" column="USR_STATUS" />
        <property name="usrName" type="java.lang.String" column="USR_NAME" />
        <property name="usrEmail" type="java.lang.String" column="USR_EMAIL" />
        <property name="usrMobile" type="java.lang.String" column="USR_MOBILE" />
        <property name="username" type="java.lang.String" column="username" />
        <property name="timeStamp" type="java.util.Date" column="TIME_STAMP" />
    </class>

    <class
            name="eu.pms.login.database.SecUserRole"
            table="sec_user_role"
    >
        <composite-id name="compId" class="eu.pms.login.database.SecUserRolePK">
            <key-property name="userId" column="pro_id" type="java.lang.String"/>
            <key-property name="roleId" column="imp_id" type="int"/>
        </composite-id>
        <!-- associations -->
        <!-- bi-directional many-to-one association to SecRole -->
        <many-to-one
                name="secRole"
                class="eu.pms.login.database.SecRole"
                not-null="true"
        >
            <column name="ROL_ID"/>
        </many-to-one>
        <!-- bi-directional many-to-one association to SecUser -->
        <many-to-one
                name="secUser"
                class="eu.pms.login.database.SecUser"
                not-null="true"
        >
            <column name="USR_ID"/>
        </many-to-one>

    </class>

